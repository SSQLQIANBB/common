{
  "name": "personal-utils",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "files": [
    "lib",
    "types"
  ],
  "main": "./lib/index.umd.js",
  "module": "./lib/index.esm.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./lib/index.esm.js",
      "require": "./lib/index.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:types": "tsc --emitDeclarationOnly",
    "example:install": "pnpm install --filter personal-utils-example",
    "example:dev": "pnpm dev --filter personal-utils-example",
    "example:build": "pnpm build --filter personal-utils-example",
    "example:preview": "pnpm preview --filter personal-utils-example",
    "example:setup": "pnpm install && pnpm build && pnpm example:dev"
  },
  "devDependencies": {
    "@types/qs": "^6.14.0",
    "qs": "^6.14.0",
    "typescript": "~5.9.2",
    "vite": "^7.0.6"
  },
  "dependencies": {
    "axios": "^1.11.0"
  }
}
